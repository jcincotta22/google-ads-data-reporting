<h1>Google Ads Data</h1>

 <table border="1">
    <tr>
      <th>Account</th>
      <th>Impresisons</th>
      <th>Cost</th>
      <th>Clicks</th>
    </tr>
      <tr>
        <td><%= @account_collection.first %></td>
        <td><%= @ibm_impressions %></td>
      </tr>
      <tr>
        <td><%= @account_collection[1] %></td>
        <td><%= @apple_impressions %></td>
      </tr>
      <tr>
        <td><%= @account_collection[2] %></td>
        <td><%= @paint_impressions %></td>
      </tr>
      <tr>
        <td><%= @account_collection[3] %></td>
        <td><%= @google_impressions %></td>
      </tr>
</table>

<%= form_tag google_datas_path, method: 'get' do %>
  <div>
    <%= select_tag(:account, options_for_select(@account_collection)) %>
    <%= submit_tag "Create Table", name: nil %>
  </div>
<% end %>
